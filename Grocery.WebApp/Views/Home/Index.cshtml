<div ng-controller="groceryController as ctrl">
    <button id="btnListar" ng-click="ctrl.Listar()">Listar produtos</button>
    @*<div data-ng-view></div>*@
   
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Country</th>
        </tr>
        <tr ng-repeat="prod in produtos">
            <td>{{prod.id}}</td>
            <td>{{prod.nome}}</td>
            <td>{{prod.valor}}</td>
        </tr>
    </table>
</div>



